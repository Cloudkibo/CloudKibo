#cssmenu
  ul
    margin 0
    padding 7px 6px 0
    background #7d7d7d repeat-x 0 -110px
    line-height 100%
    font normal 0.5333333333333333em Arial, Helvetica, sans-serif
    #width 100%
    ul
      display none
      margin 0
      padding 0
      width 185px
      position absolute
      top 40px
      left 0
      background repeat-x 0 0
      a
        &:hover
          background #7d7d7d repeat-x 0 -100px !important
          color #fff !important
      li
        float none
        margin 0
        padding 3px
    &:after
      content '.'
      display block
      clear both
      visibility hidden
      line-height 0
      height 0
  li
    margin 0 5px
    padding 0 0 8px
    float left
    position relative
    list-style none
    &:hover
      & > ul
        display block
  a
    &:hover
      background #000
      color #fff

#cssmenu a,
#cssmenu a:link
  font-weight bold
  font-size 13px
  color #e7e5e5
  display block
  padding 8px 20px
  margin 0

#cssmenu .active a,
#cssmenu li:hover > a
  background #979797 repeat-x 0 -40px
  background #666666 repeat-x 0 -40px
  color #fff

#cssmenu ul ul li:hover a,
#cssmenu li:hover li a
  background none
  border none
  color #666

#cssmenu ul ul a,
#cssmenu ul ul a:link
  font-weight normal
  font-size 12px

*
