.arrow
  cursor pointer
  display block
  height 64px
  margin-top -35px
  outline medium none
  position absolute
  top 50%
  width 64px
  z-index 5
  &.prev
    background-image url("/images/prev.png")
    left 20px
    opacity 0.2
    transition all 0.2s linear 0s
    &:hover
      opacity 1
  &.next
    background-image url("/images/next.png")
    opacity 0.2
    right 20px
    transition all 0.2s linear 0s
    &:hover
      opacity 1

